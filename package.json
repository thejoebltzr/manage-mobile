{
  "name": "SnapManage",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lintfix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "npm run update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "e2e:build:ios": "detox build --configuration ios",
    "e2e:test:ios": "npm run e2e:clean-framework; DETOX_CONFIGURATION=ios npx cucumber-js",
    "e2e:clean-framework": "detox clean-framework-cache; detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^6.1.2",
    "@react-native-community/geolocation": "^3.0.1",
    "@react-native-community/netinfo": "^9.3.6",
    "@react-native-community/slider": "^4.2.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.4.4",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@react-navigation/stack": "^6.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@rneui/base": "^4.0.0-rc.6",
    "@rneui/themed": "^4.0.0-rc.6",
    "@splitsoftware/splitio-react-native": "^0.6.0",
    "@testing-library/react-native": "^11.5.0",
    "axios": "^0.27.2",
    "eslint-plugin-prettier": "^4.2.1",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-config": "^1.4.6",
    "react-native-element-dropdown": "^2.3.0",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-i18n": "^2.0.15",
    "react-native-image-modal": "^2.0.4",
    "react-native-reanimated": "^2.10.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@cucumber/cucumber": "^8.9.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "6.3",
    "@storybook/addon-controls": "6.3",
    "@storybook/addon-essentials": "6.3",
    "@storybook/addon-links": "6.3",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.5",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/react": "6.3",
    "@storybook/react-native": "^6.0.1-beta.5",
    "@storybook/testing-library": "^0.0.9",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^19.13.0",
    "eslint": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "patch-package": "^6.4.7",
    "react-test-renderer": "18.1.0",
    "reactotron-react-native": "^5.0.3",
    "reactotron-redux": "^3.1.3",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/.jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native|react-navigation|@react-navigation|@rneui/.*))"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  }
}
